module negate(
  input wire clk,
  input wire [7:0] x,
  output wire [7:0] out
);
  // ===== Pipe stage 0:

  // Registers for pipe stage 0:
  reg [7:0] p0_x;
  always @ (posedge clk) begin
    p0_x <= x;
  end

  // ===== Pipe stage 1:
  wire [7:0] neg_6;
  assign neg_6 = -p0_x;

  // Registers for pipe stage 1:
  reg [7:0] p1_neg_6;
  always @ (posedge clk) begin
    p1_neg_6 <= neg_6;
  end
  assign out = p1_neg_6;
endmodule
