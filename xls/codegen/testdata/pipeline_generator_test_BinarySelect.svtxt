module BinarySelect(
  input wire clk,
  input wire s,
  input wire [7:0] x,
  input wire [7:0] y,
  output wire [7:0] out
);
  // ===== Pipe stage 0:

  // Registers for pipe stage 0:
  reg p0_s;
  reg [7:0] p0_y;
  reg [7:0] p0_x;
  always_ff @ (posedge clk) begin
    p0_s <= s;
    p0_y <= y;
    p0_x <= x;
  end

  // ===== Pipe stage 1:
  wire [7:0] p1_sel_4_comb;
  assign p1_sel_4_comb = p0_s == 1'h0 ? p0_y : p0_x;

  // Registers for pipe stage 1:
  reg [7:0] p1_sel_4;
  always_ff @ (posedge clk) begin
    p1_sel_4 <= p1_sel_4_comb;
  end
  assign out = p1_sel_4;
endmodule
