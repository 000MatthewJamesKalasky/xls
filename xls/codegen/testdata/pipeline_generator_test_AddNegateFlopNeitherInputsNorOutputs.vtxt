module AddNegateFlopNeitherInputsNorOutputs(
  input wire clk,
  input wire [7:0] x,
  input wire [7:0] y,
  output wire [7:0] out
);
  // ===== Pipe stage 0:
  wire [7:0] add_7;
  assign add_7 = x + y;

  // Registers for pipe stage 0:
  reg [7:0] p0_add_7;
  always @ (posedge clk) begin
    p0_add_7 <= add_7;
  end

  // ===== Pipe stage 1:
  assign out = -p0_add_7;
endmodule
